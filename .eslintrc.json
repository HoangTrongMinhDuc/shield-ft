{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "google",
        "eslint:recommended",
        "plugin:prettier/recommended",
        "plugin:react/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "require-jsdoc" : 0,
        "prettier/prettier": "error",
        "object-curly-spacing": 0
    }
}